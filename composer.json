{
  "type": "symfony-bundle",
  "name": "kml/doctrine-truncate",
  "description": "Sample symfony bundle to clear your database tables in dev mode",
  "license": "MIT",
  "keywords": [
    "doctrine-truncate",
    "truncate",
    "symfony",
    "bundle"
  ],
  "homepage": "https://github.com/komlanogbonin/doctrine-truncate",
  "authors": [
    {
      "name": "Komlan Ogbonin"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "haydenpierce/class-finder": "^0.3.1",
    "symfony/console": "^4.2",
    "symfony/framework-bundle": "^4.2",
    "symfony/orm-pack": "^1.0",
    "symfony/yaml": "4.2.*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Kml\\DoctrineTruncateBundle\\": "src/"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Kml\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    },
    "symfony": {
      "allow-contrib": "true"
    }
  },
  "require-dev": {
    "symfony/phpunit-bridge": "4.2.*",
    "symfony/var-dumper": "4.2.*"
  }
}
